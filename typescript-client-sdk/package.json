{
  "name": "openapi",
  "version": "2.2.0",
  "author": "Speakeasy",
  "type": "module",
  "tshy": {
    "sourceDialects": [
      "openapi/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./sdk/types": "./src/sdk/types/index.ts",
      "./sdk/models/errors": "./src/sdk/models/errors/index.ts",
      "./sdk/models/shared": "./src/sdk/models/shared/index.ts",
      "./sdk/models/operations": "./src/sdk/models/operations/index.ts",
      "./sdk/models/callbacks": "./src/sdk/models/callbacks/index.ts",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/speakeasy-api/openapi-generation-tests.git",
    "directory": "typescript-client-sdk"
  },
  "scripts": {
    "test": "vitest run src --reporter=junit --outputFile=.speakeasy/reports/tests.xml --reporter=default",
    "check": "npm run test && npm run lint",
    "lint": "eslint --max-warnings=0 src",
    "build": "tshy",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "zod": ">= 3"
  },
  "devDependencies": {
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^18.19.3",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "tshy": "^2.0.0",
    "typescript": "^5.4.5",
    "vitest": "^3.0.2",
    "zod": "^3.23.4"
  },
  "dependencies": {
    "decimal.js": "^10.4.3",
    "jsonpath": "^1.1.1"
  }
}
